{
  "name": "bloglist",
  "version": "1.0.0",
  "private": true,
  "description": "Bloglist App",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "start:dev": "NODE_ENV=development nodemon index.js",
    "start:test": "NODE_ENV=test node index.js",
    "start:ui": "npm start --prefix frontend",
    "test": "NODE_ENV=test jest --verbose --runInBand",
    "test:ui": "npm test --prefix frontend",
    "test:e2e": "npm run test:e2e --prefix frontend",
    "test:e2e:open": "npm run test:e2e:open --prefix frontend",
    "build:ui": "npm run build --prefix frontend && cp -r frontend/build/. public/",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "author": "Joni Vainio-Kaila",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "cypress": "^9.5.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.0",
    "prop-types": "^15.8.1",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@reduxjs/toolkit": "^1.8.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.6",
    "react-scripts": "^5.0.0",
    "web-vitals": "^0.2.4"
  }
}
